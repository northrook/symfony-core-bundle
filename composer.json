{
  "name": "northrook/symfony-core-bundle",
  "description": "Symfony bundle that provides custom core Controllers, Entity Management, and Services.",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Core\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.2",
    "symfony/config": "^7.1",
    "symfony/cache": "^v7.1",
    "symfony/http-kernel": "^7.1",
    "symfony/dependency-injection": "^7.1",
    "symfony/deprecation-contracts": "@stable",
    "symfony/event-dispatcher": "^7.1",
    "symfony/security-bundle": "^7.1",
    "symfony/mailer": "^7.1",
    "symfonycasts/reset-password-bundle": "^1.21",
    "symfonycasts/verify-email-bundle": "^1.17",
    "symfony/serializer": "^7.1",
    "symfony/form": "^7.1",
    "symfony/validator": "^7.1",
    "symfony/stopwatch": "^7.1",
    "northrook/support": "dev-main",
    "northrook/elements": "dev-main",
    "northrook/types": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/favicon": "dev-main",
    "northrook/stylesheets": "dev-main",
    "northrook/symfony-latte-bundle": "dev-main",
    "northrook/symfony-components-bundle": "dev-main",
    "ext-dom": "*",
    "ext-libxml": "*",
    "symfony/translation": "^7.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "doctrine/orm": "^2.13",
    "symfony/phpunit-bridge": "^7.0",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/annotations": "^1.0",
    "symfony/web-profiler-bundle": "^7",
    "symfony/profiler-pack": "dev-main"
  },
  "replace": {
    "symfony/polyfill-php81": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  }
}