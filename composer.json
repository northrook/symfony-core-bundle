{
  "name": "northrook/symfony-core-bundle",
  "description": "Symfony bundle that provides custom core Controllers, Entity Management, and Services.",
  "type": "symfony-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\Core\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/deprecation-contracts": "^3.0",
    "symfony/http-kernel": "^7.0",
    "northrook/symfony-latte-bundle": "dev-main",
    "northrook/logger": "dev-main",
    "northrook/types": "dev-main",
    "northrook/components": "dev-main",
    "symfony/event-dispatcher": "7.1.x-dev"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "doctrine/orm": "^2.13",
    "symfony/framework-bundle": "^7.0",
    "symfony/phpunit-bridge": "^7.0",
    "doctrine/doctrine-bundle": "^2.8",
    "doctrine/annotations": "^1.0",
    "symfony/web-profiler-bundle": "^7",
    "symfony/stopwatch": "7",
    "symfony/profiler-pack": "dev-main"
  }
}
