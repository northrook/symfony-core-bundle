<!DOCTYPE html>
<html lang="{$get->language}" id="top" theme="{$get->theme}" status="init">
<head>
    <!-- Document Meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <meta n:foreach="$document->robots as $robots" n:attr="{$robots}">
    <!-- Content Tags -->
    <title>{$document->title}</title>
    <meta n:foreach="$document->content as $content" n:attr="$content">
    <meta n:foreach="$document->meta() as $meta" n:attr="$meta">

    <!-- Preload Assets -->

    <!-- Styles -->
    <link n:foreach="$document->stylesheets as $stylesheet" n:attr="$stylesheet">

    <!-- Scripts -->
    <script src="https://unpkg.com/htmx.org@1.9.12/dist/htmx.min.js"></script>
    <script n:foreach="$document->scripts as $script" n:attr="$script"></script>

    <!-- Favicon -->
    <link n:foreach="$document->favicon() as $favicon" n:attr="$favicon">

    <!-- Theme -->
    <meta name="theme-color" content="{$get->theme->color}">
    <meta name="color-scheme" content="{$get->theme->scheme}">
    <meta n:foreach="$document->meta('msapplication') as $msapplication" n:attr="{$msapplication}">

    <!-- Facebook -->

    <!-- Twitter -->

    <!-- LinkedIn -->

</head>

{clamp( )}

<body n:attr="$document->bodyAttributes">
{render( 'skip-to-content')}
{block 'body'}{/block}
{render( 'return-to-top')}
</body>
</html>